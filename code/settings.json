{
  "editor.snippetSuggestions": "none",
  "editor.tabCompletion": "on",
  "editor.glyphMargin": true,
  "editor.minimap.enabled": false,
  "editor.insertSpaces": true,
  "editor.tabSize": 4,
  "editor.cursorStyle": "line-thin",
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.formatOnSave": true,
  "workbench.startupEditor": "none",
  "todo.symbols.box": "▪",
  "workbench.activityBar.visible": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.colorCustomizations": {
    "statusBar.background": "#1A1A1A",
    "statusBar.noFolderBackground": "#212121",
    "statusBar.debuggingBackground": "#263238"
  },
  "editor.tokenColorCustomizations": {},
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "*.egg-info": true,
    "**/__pycache__": true,
    "**/.cache": true,
    "**/.coverage": true,
    "**/.DS_Store": true,
    "**/.idea": true,
    "**/.mypy_cache": true,
    "**/.pytest_cache": true,
    "**/.terraform": true,
    "**/.tox": true,
    "**/.venv": true,
    "**/.vscode": true,
    "**/*.py[co]": true,
    "vendor": true
  },
  "window.title": "${activeEditorMedium}${separator}${rootName}",
  "files.trimTrailingWhitespace": true,
  "python.linting.enabled": true,
  "python.linting.pylintPath": "pylint",
  "python.linting.pylintEnabled": true,
  "python.linting.pylintArgs": [
    "--max-line-length=120"
  ],
  "python.venvPath": "~/Library/Caches/pypoetry/virtualenvs",
  "python.pipenvPath": "/Users/mariuss/.pyenv/shims/pipenv",
  "python.pythonPath": "/Users/mariuss/.pyenv/shims/python",
  "python.linting.mypyEnabled": true,
  "python.linting.mypyPath": "/Users/mariuss/.pyenv/shims/mypy",
  "python.terminal.activateEnvironment": true,
  "python.workspaceSymbols.enabled": true,
  "python.envFile": "${workspaceFolder}/.env",
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "/Users/mariuss/.pyenv/shims/black",
  "python.formatting.blackArgs": [
    "--line-length",
    "90"
  ],
  "diffEditor.ignoreTrimWhitespace": true,
  "files.insertFinalNewline": true,
  "workbench.statusBar.visible": true,
  "workbench.statusBar.feedback.visible": false,
  "[python]": {
    "editor.formatOnPaste": false
  },
  "workbench.iconTheme": null,
  "editor.fontLigatures": true,
  "editor.lineHeight": 21,
  "editor.fontFamily": "Fira Code, Monaco, 'Courier New', monospace",
  "projectManager.groupList": true,
  "projectManager.sortList": "Name",
  "markdownlint.config": {
    "no-inline-html": {
